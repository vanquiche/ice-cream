{
	"version": 2,
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"settings": {
		"appearanceTools": true,
		"useRootPaddingAwareAlignments": true,
		"color": {
			"palette": [
				{
					"slug": "text",
					"color": "#252422",
					"name": "Text"
				},
				{
					"slug": "background",
					"color": "#fefae0",
					"name": "Background"
				},
				{
					"slug": "primary",
					"color": "#118ab2",
					"name": "Primary"
				},
				{
					"slug": "secondary",
					"color": "#ef476f",
					"name": "Secondary"
				}
			],
			"gradients": [
				{
					"slug": "",
					"gradient": "",
					"name": ""
				}
			]
		},
		"spacing": {
			"units": [ "px", "rem", "em", "%", "vw", "vh" ],
			"padding": true,
			"margin": true
		},
		"layout": {
			"contentSize": "980px",
			"wideSize": "1400px"
		},
		"typography": {
			"dropCap": false,
			"fluid": true,
			"fontFamilies": [
				{
					"name": "Serif Display",
					"slug": "serif-display",
					"fontFamily": "\"Serif Display\", serif",
					"fontFace": [
						{
							"fontFamily": "Serif Display",
							"fontWeight": "400",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"src": [
								"file:./assets/fonts/serif-display/dm-serif-display-v15-latin-regular.woff2"
							]
						}
					]
				},
				{
					"name": "Archivo Black",
					"slug": "archivo-black",
					"fontFamily": "\"Archivo Black\", sans-serif",
					"fontFace": [
						{
							"fontFamily": "Archivo Black",
							"fontWeight": "400",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"src": [
								"file:./assets/fonts/archivo/archivo-black-v21-latin-regular.woff2"
							]
						}
					]
				},
				{
					"name": "Montserrat",
					"slug": "montserrat",
					"fontFamily": "\"Montserrat\", sans-serif",
					"fontFace": [
						{
							"fontFamily": "Montserrat",
							"fontWeight": "300",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"src": [
								"file:./assets/fonts/montserrat/montserrat-v25-latin-300.woff2"
							]
						},
						{
							"fontFamily": "Montserrat",
							"fontWeight": "400",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"src": [
								"file:./assets/fonts/montserrat/montserrat-v25-latin-regular.woff2"
							]
						},
						{
							"fontFamily": "Montserrat",
							"fontWeight": "500",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"src": [
								"file:./assets/fonts/montserrat/montserrat-v25-latin-500.woff2"
							]
						},
						{
							"fontFamily": "Montserrat",
							"fontWeight": "600",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"src": [
								"file:./assets/fonts/montserrat/montserrat-v25-latin-600.woff2"
							]
						},
						{
							"fontFamily": "Montserrat",
							"fontWeight": "700",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"src": [
								"file:./assets/fonts/montserrat/montserrat-v25-latin-700.woff2"
							]
						}
					]
				}
			],
			"fontSizes": [
				{
					"slug": "small",
					"size": "0.8rem",
					"name": "Small"
				},
				{
					"slug": "medium",
					"size": "0.95rem",
					"name": "Medium"
				},
				{
					"slug": "large",
					"size": "2rem",
					"name": "Large"
				},
				{
					"slug": "extra-large",
					"size": "2.65rem",
					"name": "Extra Large"
				},
				{
					"slug": "2-extra-large",
					"size": "3.2rem",
					"name": "2Extra Large"
				}
			]
		},
		"custom": {}
	},
	"styles": {
		"color": {
			"background": "var(--wp--preset--color--background)",
			"text": "var(--wp--preset--color--text)"
		},
		"typography": {
			"fontSize": "var(--wp--preset--font-size--medium)",
			"fontFamily": "var(--wp--preset--font-family--montserrat)",
			"lineHeight": "normal"
		},
		"elements": {},
		"blocks": {
			"core/navigation-link": {
				"elements": {
					"link": {
						"typography": {
							"textDecoration": "none"
						}
					}
				}
			},
			"core/site-title": {
				"elements": {
					"link": {
						"typography": {
							"textDecoration": "none"
						}
					}
				}
			}
		}
	},
	"templateParts": [
		{
			"name": "header",
			"title": "Header",
			"area": "header"
		},
		{
			"name": "footer",
			"title": "Footer",
			"area": "footer"
		}
	],
	"customTemplates": [
		{
			"name": "",
			"title": "",
			"postTypes": [ "post", "page" ]
		}
	]
}
